{
    "ConnectionStrings": {
        "firstOne": "First Connection String",
        "secondOne": "Second Connection String"
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "InterjectLogging": {
        "UseBuiltIn": false
    },
    "Serilog": {
        "Using": [
            "Serilog.Sinks.File",
            "Serilog.Sinks.Async",
            "Serilog.Enrichers.Environment",
            "Serilog.Enrichers.Process",
            "Serilog.Enrichers.Thread",
            "Serilog.Formatting.Compact"
        ],
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "Microsoft.AspNetCore": "Warning",
                "System": "Warning"
            }
        },
        "Enrich": [
            "FromLogContext",
            "WithMachineName",
            "WithProcessId",
            "WithThreadId"
        ],
        "WriteTo": [
            {
                "Name": "Console"
            },
            {
                "Name": "Async",
                "Args": {
                    "configure": [
                        {
                            "Name": "File",
                            "Args": {
                                "path": ".\\Logs\\app-.json",
                                "rollingInterval": "Day",
                                "retainedFileCountLimit": 30,
                                "shared": true,
                                "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact"
                            }
                        }
                    ]
                }
            }
        ]
    },
    "AllowedHosts": "*",
    "Application": {
        "Name": "Interject Dotnet API",
        "Version": "1.2.0",
        "Framework": ".net8.0",
        "UseClientIdAsConnectionName": true
    },
    "Authority": "https://test-auth.gointerject.com"
}