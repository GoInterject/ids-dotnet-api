{
  "metadata": [
    {
      "src": [
        {
          "src": "../interject.api",
          "files": [
            "interject.api.csproj"
          ]
        },
        {
          "src": "../interject.data.api/interject-dotnet-api",
          "files": [
            "InterjectDotnetApi.csproj"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "source/pages/**.md",
          "source/pages/**.yml",
          "index.md",
          "toc.yml"
        ],
        "exclude": [
          "obj/**",
          "site/**"
        ],
        "flatten": true
      }
    ]
    ,
    "resource": [
      {
        "files": [
          "source/static/**"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ],
        "flatten": true 
      }
    ],
    "dest": "site",
    "globalMetadata": {
      "_appTitle": "Your Project Title",
      "_appFooter": "Your Footer Information",
      "_enableSearch": true,
      "_enableGitHub": true
    },
    "overwrite": [],
    "template": [
      "default"
    ]
  },
  "serve": {
    "port": 8080
  }
}
